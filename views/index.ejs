    <section id="chat">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-6" id="chatPanel">
                        <h1 style="margin-bottom: 4vh">Car Dashboard Bot</h1>
                    <div id="chatDiv">
                        <p class="watson"> <%= message %></p>

                    </div>
                    <div id="formDiv">
                        <form id="chatForm">
                            <input type="text" name="message" id="message" placeholder="Enter your message here." autocomplete="off">
                        </form>
                    </div>
                </div>
                <div class="col-md-6" id="resultsPanel">

                </div>
            </div>
        </div>
    </section>

    <script src="https://code.jquery.com/jquery-3.2.1.min.js " integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
